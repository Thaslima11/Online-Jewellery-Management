        <servlet-name>Studs</servlet-name>
        <url-pattern>/Studs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChianController</servlet-name>
        <url-pattern>/ChianController</url-pattern>
    </servlet-mapping>
