        <servlet-name>VerifyUser</servlet-name>
        <url-pattern>*.user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ring</servlet-name>
        <url-pattern>/Ring</url-pattern>
    </servlet-mapping>
